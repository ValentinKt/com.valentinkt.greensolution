<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="navBarRowModel" type="com.valentinkt.sunapp.modules.navbar.data.model.NavBarRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowtwelve"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_weight="0.96" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVector"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_28pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_amber_300"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_300"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNutritionalSum"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.83" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{navBarRowModel.txtNutritionalSum}"
                style="@style/txtIntermedium14" tools:text="@string/msg_nutritional_sum"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtTwelve"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_82pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.04" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{navBarRowModel.txtTwelve}"
            style="@style/txtPoppinsmedium16_3" tools:text="@string/lbl3"/>
    </LinearLayout>
</layout>
