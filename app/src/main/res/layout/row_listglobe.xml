<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listglobeRowModel" type="com.valentinkt.sunapp.modules.myaccount.data.model.ListglobeRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowfive"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowglobe"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_weight="0.93" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageGlobe"
                android:layout_height="@dimen/_20pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_globe"
                tools:ignore="ContentDescription" tools:src="@drawable/img_globe"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.77" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listglobeRowModel.txtLanguage}"
                style="@style/txtIntermedium14" tools:text="@string/lbl_language"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtFive"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_153pxh"
            android:layout_weight="0.07" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{listglobeRowModel.txtFive}"
            style="@style/txtPoppinsmedium16_3" tools:text="@string/lbl3"/>
    </LinearLayout>
</layout>
