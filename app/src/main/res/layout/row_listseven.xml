<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listsevenRowModel" type="com.valentinkt.sunapp.modules.myaccount.data.model.ListsevenRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackseven"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <TextView android:ellipsize="end" android:id="@+id/txtSeven"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listsevenRowModel.txtSeven}"
            style="@style/txtPoppinsmedium16_3" tools:text="@string/lbl3"/>
        <LinearLayout android:id="@+id/linearRowclock"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageClock"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock_black_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_clock_black_900"/>
            <TextView android:ellipsize="end" android:id="@+id/txtAbout"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_weight="0.69" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listsevenRowModel.txtAbout}"
                style="@style/txtIntermedium14" tools:text="@string/lbl_about"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRoweight"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowclockOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.91" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageClockOne"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAboutOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listsevenRowModel.txtAboutOne}"
                    style="@style/txtIntermedium14" tools:text="@string/lbl_about"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtEight"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_179pxh"
                android:layout_weight="0.09" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listsevenRowModel.txtEight}"
                style="@style/txtPoppinsmedium16_3" tools:text="@string/lbl3"/>
        </LinearLayout>
    </FrameLayout>
</layout>
